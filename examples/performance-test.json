def add(array)
    [for ($array) . + 2]

def sub(array)
    [for ($array) . - (2 + 2 * 16 * 2 - 54)]
    
def mult(array)
    [for ($array) . * 2]
    
def div(array)
    [for ($array) . / 2]
    
